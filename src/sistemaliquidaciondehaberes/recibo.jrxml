	<queryString>
		<![CDATA[SELECT
     *,
     empresas.`clase` AS empresas_clase,
     empresas.`cuit` AS empresas_cuit,
     empresas.`razonSocial` AS empresas_razonSocial,
     empresas.`direccion` AS empresas_direccion,
     empresas.`barrio` AS empresas_barrio,
     persona.`apellido` AS persona_apellido,
     persona.`nombre` AS persona_nombre,
     persona.`direccion` AS persona_direccion,
     persona.`tipoDoc` AS persona_tipoDoc,
     persona.`nroDoc` AS persona_nroDoc,
     persona.`cuil` AS persona_cuil
FROM
     `empresas` empresas,
